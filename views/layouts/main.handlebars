  <link rel="stylesheet" href="/css/style.css">
  <link rel="icon" href="/assets/apple-icon-180x180.png" />
  <title>Tek Tok</title>
</head>

<body>
  <div class="justify-space-between">
    <header class="row display-flex justify-space-between p-2">

      <div class="col-7 animate__animated animate__fadeInRightBig">
        <object id="svg1" data="/assets/tektokyellowtext.svg" type="image/svg+xml" width="100" height="100"></object>
      </div>

      <nav class="display-flex col-5 align-items-center">
        {{!-- <div class="animate__animated animate__fadeInRight"> --}}
          <div>
            <ul class="row justify-content-between nav-list">
              <li class="col-1 nav-item animate__animated animate__fadeInRightBig">
                <a href="/" class="a">Home</a>
              </li>
              <li class="col-2 mx-2 nav-item animate__animated animate__fadeInRightBig">
                <a href="/resources" class="a">Resources</a>
              </li>
              <li class="col-1 nav-item animate__animated animate__fadeInRightBig">
                <a href="/games" class="a">Games</a>
              </li>
              <li class="col-2 mx-2 nav-item animate__animated animate__fadeInRightBig">
                <a href="/profile" class="a">Profile</a>
              </li>

              {{#if logged_in}}
              <li class="col-1 animate__animated animate__fadeInRightBig">
                <button class="no-button" id="logout">Logout</button>
              </li>
              {{else}}
              <!-- else -->
              <li class="col-1 animate__animated animate__fadeInRightBig">
                <a href="/login" class="a">Login</a>
              </li>
              {{/if}}
              <!-- end if -->
              {{!--
              </li>
            </ul> --}}
            </ul>
          </div>
      </nav>
    </header>

    <main class="justify-space-between row">
      {{{body}}}
